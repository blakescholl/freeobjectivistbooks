<div class="event">
    <p class="headline"><%= render partial: "admin/events/event_headline", locals: {event: event} %></p>
    <% if event.message.present? %><p class="reason"><%= format_block event.message %></p><% end %>
    <p class="timestamp"><%= l event.created_at %></p>
</div>
<div class="clear"></div>
